    "require": {
        "php": ">=5.4.0",
        "kijin/pinboard-api": "0.3.*",
        "zendframework/zend-servicemanager": "2.3.*",
        "zendframework/zend-http": "2.3.*",
        "zendframework/zend-config": "2.3.*"
    },
    "require-dev": {
